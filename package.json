{
  "name": "DffooDatabase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "adb reverse tcp:8081 tcp:8081 && application=app react-native start --reset-cache",
    "test": "jest",
    "prestorybook": "rnstl",
    "storybook": "./scripts/start_storybook.sh",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc",
    "watch": "yarn run build -w",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start --reset-cache\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.3.4",
    "@react-native-community/netinfo": "~2.0.10",
    "@react-native-community/viewpager": "^1.1.6",
    "prop-types": "~15.7.2",
    "ramda": "~0.26.1",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-native-cached-image": "~1.4.3",
    "react-native-charts-wrapper": "^0.5.2",
    "react-native-elements": "~1.1.0",
    "react-native-fast-image": "^6.0.0",
    "react-native-vector-icons": "~6.4.2",
    "react-redux": "~7.0.3",
    "react-router-native": "^5.0.0",
    "redux": "~4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "~5.6.0",
    "redux-thunk": "~2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.4.4",
    "@babel/runtime": "~7.4.4",
    "@storybook/react-native": "^4.1.18",
    "@types/jest": "^24.0.13",
    "@types/ramda": "^0.26.8",
    "@types/react": "^16.8.17",
    "@types/react-native": "^0.57.57",
    "@types/react-router-native": "^4.2.4",
    "@types/react-test-renderer": "^16.8.1",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^4.1.0",
    "eslint": "~5.16.0",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-plugin-import": "~2.17.2",
    "eslint-plugin-jsx-a11y": "~6.2.1",
    "eslint-plugin-react": "~7.13.0",
    "jest": "~24.8.0",
    "metro-react-native-babel-preset": "~0.54.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.3",
    "redux-devtools-extension": "~2.13.8",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
